{
  "manifest_version": 2,
  "name": "peek stock",
  "description": "real-time TW stock crawler",
  "version": "1.0",
  "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html",
     "default_title": "Peek!"
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ]
}